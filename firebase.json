{
  "hosting": {
    "public": "out",
    "rewrites": [
      {
        "source": "/home/**",
        "destination": "/app/home/[id]/page.html"
      },
      {
        "source": "/profile/**",
        "destination": "/app/profile/[uid]/page.html"
      },
      {
        "source": "/main-app/dashboard/**",
        "destination": "/app/main-app/dashboard/index.html"
      },
      {
        "source": "/main-app/chat/**",
        "destination": "/app/main-app/chat/index.html"
      },
      {
        "source": "/main-app/**",
        "destination": "/app/main-app/page.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}